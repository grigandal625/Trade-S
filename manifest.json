{
	"manifest_version": 2,
	"name": "Trade-S",
	"description": "Auto signaler",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon_128.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"webRequest",
		"storage",
		"https://api.vk.com/"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}