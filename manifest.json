{
	"manifest_version": 2,
	"name": "Olympcepy",
	"description": "Signal event listener to OlympTrade",
	"version": "5.0",
	"browser_action": {
		"default_icon": "icon_128.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"webRequest",
		"storage",
		"https://api.vk.com/"
	]
}